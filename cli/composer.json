{
	"name": "eventum/eventum-cli",
	"description": "Eventum Issue / Bug tracking system",
	"homepage": "https://github.com/eventum/eventum",
	"license": "GPL-2.0",
	"authors": [
		{
			"name": "Elan RuusamÃ¤e",
			"email": "glen@delfi.ee",
			"role": "Developer"
		},
		{
			"name": "Bryan Alsdorf",
			"email": "balsdorf@gmail.com",
